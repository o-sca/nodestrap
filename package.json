{
  "name": "@o-sca/nodestrap",
  "version": "1.0.8",
  "description": "bootstrap javascript/typescript project template generator",
  "main": "dist/index.js",
  "typings": "dist/index",
  "bin": {
    "nodestrap": "dist/index.js"
  },
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.ts'",
    "start": "ts-node src/index.ts",
    "prepublishOnly": "npm run clean && tsc -p .",
    "compile": "npm run clean && npm test && tsc -p .",
    "watch": "tsc -w -p .",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/o-sca/nodestrap.git"
  },
  "keywords": [],
  "author": "Oscar Yang Guang Zhu <ozhu35@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/ejs": "^3.1.1",
    "@types/inquirer": "^9.0.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.15",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "chai": "^4.3.7",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.15.16",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^10.2.0",
    "path": "^0.12.7",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "arg": "^5.0.2",
    "chalk": "^4.0.0",
    "ejs": "^3.1.8",
    "inquirer": "^8.0.0",
    "shelljs": "^0.8.5"
  },
  "files": [
    "dist",
    "templates"
  ]
}
