{
    "name": "@o-sca/nodestrap",
    "version": "1.1.0",
    "description": "bootstrap javascript/typescript project template generator",
    "repository": {
        "type": "git",
        "url": "https://github.com/o-sca/nodestrap.git"
    },
    "keywords": [],
    "author": "Oscar Yang Guang Zhu <ozhu35@gmail.com>",
    "license": "MIT",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.cts",
    "exports": {
        "require": {
            "types": "./dist/index.d.cts",
            "default": "./dist/index.cjs"
        },
        "import": {
            "types": "./dist/index.d.mts",
            "default": "./dist/index.mjs"
        }
    },
    "bin": {
        "nodestrap": "./dist/index.js"
    },
    "scripts": {
        "build": "pkgroll --minify",
        "test": "tsx tests/index.spec.ts",
        "start": "tsx src/index.ts",
        "prepare": "husky install"
    },
    "dependencies": {
        "@inquirer/prompts": "^1.2.3",
        "@types/ejs": "^3.1.1",
        "@types/shelljs": "^0.8.11",
        "arg": "^5.0.2",
        "chalk": "^4.0.0",
        "ejs": "^3.1.8"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.4.3",
        "@commitlint/config-conventional": "^18.4.3",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^9.2.6",
        "@types/inquirer": "^9.0.3",
        "@types/node": "^18.11.15",
        "@typescript-eslint/eslint-plugin": "^5.59.7",
        "@typescript-eslint/parser": "^5.59.7",
        "esbuild": "^0.15.16",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "lint-staged": "^15.0.2",
        "manten": "^1.1.0",
        "pkgroll": "^2.0.1",
        "prettier": "^2.8.1",
        "tsx": "^3.14.0",
        "typescript": "^4.9.4",
        "husky": "^8.0.0"
    },
    "files": [
        "dist",
        "templates"
    ]
}
