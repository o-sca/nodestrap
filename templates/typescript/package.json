{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start:dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:prod": "node dist",
    "build:win32": "rmdir /s dist && npm test && tsc -p .",
    "build:darwin": "rm -rf && npm test && tsc",
    "watch": "tsc -w -p .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "keywords": [],
  "author": "<%= authorName %>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.11.15",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "jest": "^29.5.0",
    "chai": "^4.3.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^10.2.0",
    "path": "^0.12.7",
    "prettier": "^2.8.1",
    "ts-jest": "29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.9.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
