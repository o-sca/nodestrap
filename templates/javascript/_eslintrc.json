{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"plugins": ["simple-import-sort", "unused-imports"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"unused-imports/no-unused-imports": "error",
		"simple-import-sort/imports": "error",
		"simple-import-sort/exports": "error"
	}
}
